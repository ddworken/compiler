def f1(a, b, c) :
    if a == 0:
        let _1 = print(-1) in 
        f2(print(b), print(c), print(a))
    else: 
        f1(a - 1, b, c)

and def f2(b, c, a) :
    if b == 0:
        let _1 = print(-1) in 
        f3(print(c), print(a), print(b))
    else:
        f2(b - 1, c, a)

and def f3(c, a, b) :
    if c == 0:
        let _ = print(-1) in 
        print(a) + print(b) + print(c)
    else:
        f3(c - 1, a, b)

