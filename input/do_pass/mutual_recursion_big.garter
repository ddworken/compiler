def f1(a, b, c, d) :
    if a == 1:
        f2(a, b, c, d)
    else:
        f1(a - 1, b, c, d)

and def f2(a, b, c, d) :
    if b == 2:
        f3(a, b, c, d)
    else:
        f2(a, b - 1, c, d)

and def f3(a, b, c, d) :
    if c == 3:
        f4(a, b, c, d)
    else:
        f3(a, b, c - 1, d)

and def f4(a, b, c, d) :
    if d == 4:
        a + b + c + d
    else:
        f4(a, b, c, d - 1)

f1(10, 12, 9, 13) 
